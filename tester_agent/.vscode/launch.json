{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Tester Agent Script",
      "type": "debugpy",
      "request": "launch",
      "module": "tester_agent.run_tests",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "justMyCode": true,
      "env": {
        "A2A_REGISTRY_URL": "http://localhost:8090"
      }
    },
    {
      "name": "Tester Agent REPL",
      "type": "debugpy",
      "request": "launch",
      "module": "tester_agent.repl",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "justMyCode": true,
      "env": {
        "A2A_REGISTRY_URL": "http://localhost:8090"
      }
    }
  ]
}